{
  "name": "react-phylocanvas",
  "version": "0.0.3",
  "description": "ReactJS / Phylocanvas integration. Draw phylocanvas trees in your react app.",
  "main": "lib/PhylocanvasComponent.js",
  "author": "Ben Jeffery",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benjeffery/react-phylocanvas.git"
  },
  "dependencies": {
    "lodash.keys": "^4.2.0",
    "phylocanvas": "git+https://github.com/phylocanvas/phylocanvas.git#v2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-react": "^7.3.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.1.0",
    "gulp-git": "^2.4.2",
    "gulp-util": "^3.0.7"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "browserify-shim": {
    "react": "global:React"
